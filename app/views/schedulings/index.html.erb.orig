<div class="container marg2">
    <br>
    <br><br>
        <div class="row">
            <div class="col-lg-12 ">

<h1>Listado de programaciones</h1>

  <div class="panel panel-default">
      <!-- Default panel contents -->
      <div class="panel-heading">Lista de programaciones</div>
      <div class="panel-body">
        <p class="center"> En esta sección es posible modificar las el listado de programaciones.<br>
        
        </p>
      </div>

      <!-- Table -->
      <table class="table">

  <thead>
    <tr>
      <th>Id oferta vehículo</th>
      <th>Rut conductor</th>
      <th>Rut cliente</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @schedulings.each do |scheduling| %>
      <tr>
        <td><%= scheduling.id_off_veh %></td>
        <td><%= scheduling.run_driv %></td>
        <td><%= scheduling.run_cli %></td>
        <td><%= button_to 'Ver', scheduling  , :method => 'get', :class => 'btn btn-info'%></td>
        <td><%= button_to 'Editar', edit_scheduling_path(scheduling) , :method => 'get', :class => 'btn btn-warning'%></td>
        <td><%= button_to 'Eliminar', scheduling, method: :delete, data: { confirm: '¿Confirma eliminación?' } , :class => 'btn btn-danger'%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div></div></div></div>

<%= button_to 'Nuevo', new_scheduling_path, :method => 'get', :class => 'btn btn-default'%>

